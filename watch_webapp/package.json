    "buffer": "^6.0.3",
    "buffer-loader": "^0.1.0",
    "epic-spinners": "^1.1.0",
    "fili": "^2.0.3",
    "readline": "^1.3.0",
    "smoothie": "^1.36.0",
    "text-encoding": "^0.7.0",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vuetify": "^2.6.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
