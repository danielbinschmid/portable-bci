  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-crosswalk-webview-v3": {},
      "cordova-plugin-bluetoothle": {}
